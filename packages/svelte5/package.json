	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./themes": "./dist/registry/themes.css"
	},
	"peerDependencies": {
		"lucide-svelte": "^0.454.0",
